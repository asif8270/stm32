#ifndef I2C_H_
#define I2C_H_

#define GPIOB_EN		(1U<<2)
#define I2C1_EN			(1U<<21)
#define I2C1_PE			(1U<<0)
#define I2C1_100KHz		80
#define SD_MODE_MAX_TR	17

#define I2C1_ST			(1U<<8)
#define I2C1_SB			(1U<<0)
#endif /* I2C_H_ */
