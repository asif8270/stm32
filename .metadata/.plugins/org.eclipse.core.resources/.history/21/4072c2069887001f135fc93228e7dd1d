#include <stdio.h>
#include <stdint.h>
#include "stm32f4xx.h"
#include "lis3dsh.h"
#include "adc.h"
#include "systick.h"

#define GPIOD_EN			(1U<<3)

#define PIN12				(1U<<12)
#define LED_GREEN			PIN12

int main(void)
{
	/*Enable clock access to the GPIOD*/
	RCC->AHB1ENR |= GPIOD_EN;

	/*Set the PD12 as OUTPUT*/
	GPIOD->MODER |= (1U<<24);
	GPIOD->MODER &= ~(1U<<25);

	systickDelayMs(1000);
}
