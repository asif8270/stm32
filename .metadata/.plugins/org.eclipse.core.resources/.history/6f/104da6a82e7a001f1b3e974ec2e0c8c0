#include "stm32f4xx.h"

#define GPIOA_EN			(1U<<0)
#define USART_EN			(1U<<17)

#define PIN14				(1U<<14)
#define PIN0				(1U<<0)


int main(void)
{
	/*Enable clock access to the GPIOA*/
	RCC->AHB1ENR |= GPIOA_EN;
	RCC->APB1ENR |= USART_EN;

	/*Set the PA2 & PA3 as Alternate Function*/
	GPIOA->MODER &= ~(1U<<0);
	GPIOA->MODER &= ~(1U<<1);

	while(1)
	{
	}
}






