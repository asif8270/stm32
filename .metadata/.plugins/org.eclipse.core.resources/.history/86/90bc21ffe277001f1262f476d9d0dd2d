#include "stm32f4xx.h"
#include "stm32f407xx.h"




#define GPIOD_EN			(1U<<3)
#define PIN12				(1U<<12)
#define PIN14				(1U<<14)
#define LED_GREEN			PIN12
#define LED_RED				PIN14

int main(void)
{
	RCC->AHB1ENR |= GPIOD_EN;

	GPIOD->MODER |= (1U<<24);
	GPIOD->MODER &= ~(1U<<25);

	GPIOD->MODER |= (1U<<28);
	GPIOD->MODER &= ~(1U<<29);

	while(1)
	{
		GPIOD->BSRR = LED_GREEN;
		for (int i = 0; i < 100000; ++i) {}
		GPIOD->BSRR = (1U<<28);
		for (int i = 0; i < 100000; ++i) {}
		GPIOD->BSRR = LED_RED;
		for (int i = 0; i < 100000; ++i) {}
		GPIOD->BSRR = (1U<<30);
		for (int i = 0; i < 100000; ++i) {}
	}
}
